{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "types": ["node", "express", "./src/types/express"],
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"]
    },
    "ignoreDeprecations": "6.0",
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
